<html xmlns="http://www.w3.org/1999/xhtml" >
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<title>meta_profile</title>
<link href="meta.css" rel="stylesheet" type="text/css" />
<style>
	.selected{  color:#FF0000;}
</style>
<style>
body {scrollbar-face-color: #ffffff;
scrollbar-highlight-color: #c0c0c0;
scrollbar-3dlight-color: #FFFFFF;
scrollbar-darkshadow-color: #C0C0C0;
scrollbar-shadow-color: #ffffff;
scrollbar-arrow-color: #C0C0C0;
scrollbar-track-color: #ffffff;}
</style> 
</head>
<body oncontextmenu=self.event.returnValue=false onselectstart="return false" >
	<dl id="con">	
	@@@BODY@@@
	</dl>
</body>
</html>


<script type="text/javascript">
	function showHideModel(id,id1)
	{
    	var a=document.getElementById(id);
		var b=document.getElementById(id1);
		var img1="url(meta_close.gif)";
		var img2="url(meta_open.gif)";
		if(b.style.backgroundImage==""||b.style.backgroundImage==img1)
			b.style.backgroundImage=img2;
		else
			b.style.backgroundImage=img1;
    	if(a.style.display=="none")
        	a.style.display="block";
   		else
        	a.style.display="none";
	}	

	var tds = document.getElementById("con").getElementsByTagName("li");
	window.onload = function(){
				for( var i=0; i<tds.length; i++ ){
					tds[i].onclick = function(){
						tdonclick(this);
					}
				}
			}
	function tdonclick(obj){
				for( var o=0; o<tds.length; o++ ){
					if( tds[o] == obj ){
							tds[o].children(0).className = 'selected';
					}
					else{
						tds[o].children(0).className = '';
					}
				}
			}
</script>